{
  "manifest_version": 2,
  "name": "website-color",
  "version": "0.1",
  "description": "website-color description",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
      "theme"
  ],
  "icons": {
    "64": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
